{
    "require": {
        "pecee/simple-router": "4.3.7.2",
        "phpoffice/phpword": "v0.18.*",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "ext-xmlreader": "*",
        "ext-xmlwriter": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "ezyang/htmlpurifier": "^4.13",
        "maennchen/zipstream-php": "^2.1",
        "markbaker/complex": "^3.0",
        "markbaker/matrix": "^3.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/simple-cache": "^1.0",
        "phpoffice/phpspreadsheet": "1.20.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "3.5.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
